{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quickopenraycast",
  "title": "QuickOpenRaycast",
  "description": "Quickly open your historical projects",
  "icon": "logo.png",
  "author": "Joys",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "History",
      "description": "Quickly open your historical projects.",
      "mode": "view"
    },
    {
      "name": "config",
      "title": "Config",
      "description": "your Config.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.6",
    "licia": "^1.31.0",
    "node-fetch": "^3.0.0",
    "parse-git-config": "^3.0.0",
    "prettier": "^2.5.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
